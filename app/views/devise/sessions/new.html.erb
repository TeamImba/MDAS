<h2>Sign in</h2>

<%= form_for(resource, :as => resource_name, :url => "/users/sign_in/") do |f| %>
  <p><%= f.label :email %><br />
  <%= f.email_field :email %></p>

  <p><%= f.label :password %><br />
  <%= f.password_field :password %></p>

  <p><%= f.submit "Sign in" %></p>
<%- end %>
<%= render :partial => "devise/shared/links" %>