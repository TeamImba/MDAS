!!! 5
-ie_html :class => 'no-js', :lang => 'en' do
  %head
    = render "layouts/head", :stylesheet_partial => "layouts/stylesheets"
  %body{ :class => "home", :id => "index" }
    %section{ :class => "nav container" }
      %nav{ :class => "box container_12" }
        %section{ :id => "logo" }
          =link_to(image_tag("multiply.png"), root_url)
        %ul{ :id => "navigation" }
          %li=link_to("Home", root_url)
          %li=link_to("Shop at Multiply", "http://multiply.com/marketplace?utm_source=semmicrosite%2B&utm_medium=link%2B&utm_campaign=semmicrosite%2B", :rel => "external", :target => "_blank")
          %li=link_to("About", about_url)
          %li=link_to("FAQs", faq_url)
          %li=link_to("Promo Mechanics", promo_url)
    %section{:class => "header container"}
      %header{:class => "box container_12", :style => "height:250px;position:relative"}
        %span{:id => "dti"}
          Per DTI-NCR Permit #6788 Series of 2011. 
          =link_to("Click here for details.", promo_url)
        %aside{:class => "alpha grid_3", :style => "position:absolute;width:220px;bottom:10px;right:20px"}
          =link_to("Claim Your Shirt", registration_url, :class => 'forms claim btn', :style => "height:43px")
    #fb-root
    :javascript
      window.fbAsyncInit = function() {
        FB.init({appId: '213790775324755', status: true, cookie: true, xfbml: true});
      };
      (function() {
        var e = document.createElement('script'); e.async = true;
        e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
        document.getElementById('fb-root').appendChild(e);
      }());
    = yield
    %footer{:class => "box container_12" }
    /= render "layouts/signup"
    /= render "layouts/signup_complete"
    = render "layouts/javascripts"